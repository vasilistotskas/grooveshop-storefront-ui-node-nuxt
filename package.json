  },
  "homepage": "https://github.com/vasilistotskas/grooveshop-storefront-ui-node-nuxt#readme",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:all": "ncu -u && pnpm i && pnpm run build && pnpm run format:fix && pnpm run lint:fix && pnpm run dev",
    "prepare": "nuxt prepare",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "format": "npx prettier --check '*/**/*.{js,ts,vue,html}'",
    "format:fix": "npx prettier --write '*/**/*.{js,ts,vue,html}'",
    "cleanup": "nuxt cleanup",
    "generate:locales": "node --enable-source-maps --no-warnings=ExperimentalWarning --loader ts-node/esm tools/translator/src/app.ts",
    "analyze": "npx nuxt analyze",
    "test:dev": "nuxt dev --no-fork NUXT_VITEST_DEV_TEST=true",
    "test:unit": "vitest run",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test": "pnpm test:unit --run && pnpm test:types",
    "semantic-release": "semantic-release"
  },
