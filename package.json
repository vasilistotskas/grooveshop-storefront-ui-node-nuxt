	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"prepare": "nuxt prepare",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"start": "node .output/server/index.mjs",
		"serve": "serve dist/",
		"lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
		"lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --write --ignore-path .gitignore .",
		"cleanup": "nuxt cleanup",
		"generate:locales": "node --no-warnings=ExperimentalWarning --loader ts-node/esm tools/translator.ts ./locales en-US.yml",
		"analyze": "npx nuxt analyze",
		"test:dev": "NUXT_VITEST_DEV_TEST=true nuxt dev",
		"test:unit": "vitest",
		"test:types": "nuxt prepare && vue-tsc --noEmit",
		"test": "npm test:unit --run && npm test:dev && npm test:types",
		"semantic-release": "semantic-release"
	},
