{
  "name": "@vasilistotskas/grooveshop-storefront-ui-node-nuxt",
  "version": "3.45.1",
  "private": false,
  "description": "Grooveshop Storefront UI Node Nuxt",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasilistotskas/grooveshop-storefront-ui-node-nuxt.git"
  },
  "author": "vasilistotskas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasilistotskas/grooveshop-storefront-ui-node-nuxt/issues"
  },
  "homepage": "https://github.com/vasilistotskas/grooveshop-storefront-ui-node-nuxt#readme",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:all": "ncu -u && npm i && npm run build && npm run lint && npm run dev",
    "prepare": "nuxt prepare",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "lint": "eslint . --fix",
    "lint:ci": "eslint . --cache",
    "cleanup": "nuxt cleanup",
    "analyze": "npx nuxt analyze",
    "test": "vitest run",
    "test:ci": "vitest run --project=unit --project=nuxt --coverage",
    "test:coverage": "vitest run --coverage",
    "openapi-ts": "openapi-ts",
    "semantic-release": "semantic-release",
    "generate:schema": "node scripts/fetch-schema.mjs"
  },
  "dependencies": {
    "@grooveshop/recommendation-engine-client": "^1.0.0",
    "@internationalized/date": "3.10.0",
    "@nuxt/eslint": "1.11.0",
    "@nuxt/fonts": "0.12.1",
    "@nuxt/icon": "2.1.0",
    "@nuxt/image": "1.11.0",
    "@nuxt/scripts": "0.13.0",
    "@nuxt/test-utils": "3.20.1",
    "@nuxt/ui": "4.2.1",
    "@nuxtjs/device": "3.2.4",
    "@nuxtjs/i18n": "10.2.1",
    "@nuxtjs/seo": "3.2.2",
    "@pinia/nuxt": "0.11.3",
    "@tailwindcss/typography": "0.5.19",
    "@vueuse/nuxt": "14.1.0",
    "coveralls-next": "6.0.1",
    "defu": "6.1.4",
    "h3": "1.15.4",
    "hookable": "5.5.3",
    "magic-string": "0.30.21",
    "nuxt": "4.2.1",
    "nuxt-auth-utils": "0.5.25",
    "ofetch": "1.5.1",
    "pinia": "3.0.4",
    "redis": "5.10.0",
    "rss": "1.2.2",
    "tailwindcss": "4.1.17",
    "ufo": "1.6.1",
    "unstorage": "1.17.3",
    "vue": "3.5.25",
    "vue-i18n": "11.2.2",
    "vue-i18n-routing": "1.2.0",
    "vue-router": "4.6.3",
    "zod": "4.1.13"
  },
  "devDependencies": {
    "@headlessui/vue": "1.7.23",
    "@hey-api/openapi-ts": "0.88.0",
    "@iconify-json/ant-design": "1.2.5",
    "@iconify-json/fa-solid": "1.2.2",
    "@iconify-json/fa6-solid": "1.2.4",
    "@iconify-json/heroicons": "1.2.3",
    "@iconify-json/heroicons-outline": "1.2.1",
    "@iconify-json/heroicons-solid": "1.2.1",
    "@iconify-json/lucide": "1.2.77",
    "@iconify-json/mdi": "1.2.3",
    "@iconify-json/unjs": "1.2.3",
    "@nuxt/devtools": "3.1.1",
    "@nuxt/kit": "4.2.1",
    "@nuxt/schema": "4.2.1",
    "@nuxt/types": "2.18.1",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "12.0.2",
    "@semantic-release/npm": "13.1.2",
    "@semantic-release/release-notes-generator": "14.1.0",
    "@stripe/stripe-js": "8.5.3",
    "@testing-library/jest-dom": "6.9.1",
    "@types/node": "24.10.1",
    "@types/rss": "0.0.32",
    "@vitest/coverage-v8": "4.0.4",
    "@vitest/ui": "4.0.4",
    "@vue/test-utils": "2.4.6",
    "@vueuse/core": "14.1.0",
    "@vueuse/integrations": "14.1.0",
    "@vueuse/shared": "14.1.0",
    "eslint": "9.39.1",
    "eslint-plugin-better-tailwindcss": "3.7.11",
    "happy-dom": "20.0.11",
    "semantic-release": "25.0.2",
    "typescript": "5.9.3",
    "vite": "7.2.4",
    "vite-plugin-eslint2": "5.0.4",
    "vitest": "4.0.4",
    "vitest-environment-nuxt": "1.0.1",
    "vue-tsc": "3.1.5"
  },
  "publishConfig": {
    "@vasilistotskas:registry": "https://npm.pkg.github.com"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": ".nuxt/dist/client",
              "label": "Client"
            },
            {
              "path": ".nuxt/dist/server",
              "label": "Server"
            },
            {
              "path": ".output",
              "label": "Output"
            }
          ]
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false,
          "tarballDir": ".output"
        }
      ]
    ]
  },
  "packageManager": "pnpm@10.24.0",
  "pnpm": {
    "overrides": {
      "sharp": "0.33.4"
    },
    "onlyBuiltDependencies": [
      "@intlify/vue-i18n-bridge",
      "@intlify/vue-router-bridge",
      "@parcel/watcher",
      "esbuild",
      "sharp",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
