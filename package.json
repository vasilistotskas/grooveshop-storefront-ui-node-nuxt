{
  "name": "@vasilistotskas/grooveshop-storefront-ui-node-nuxt",
  "version": "1.40.0",
  "private": false,
  "description": "Grooveshop Storefront UI Node Nuxt",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/vasilistotskas/grooveshop-storefront-ui-node-nuxt.git"
  },
  "author": "vasilistotskas",
  "license": "MIT",
  "homepage": "https://grooveshop.site",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "prepare": "nuxt prepare",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
    "format": "npx prettier --check '*/**/*.{js,ts,vue,html}'",
    "format:fix": "npx prettier --write '*/**/*.{js,ts,vue,html}'",
    "cleanup": "nuxt cleanup",
    "generate:locales": "node --no-warnings=ExperimentalWarning --loader ts-node/esm tools/translator.ts ./locales en-US.yml",
    "analyze": "npx nuxt analyze",
    "test:dev": "nuxt dev --no-fork NUXT_VITEST_DEV_TEST=true",
    "test:unit": "vitest run",
    "test:types": "nuxi prepare && vue-tsc --noEmit",
    "test": "pnpm test:unit --run && pnpm test:types",
    "semantic-release": "semantic-release"
  },
