{
	"name": "@vasilistotskas/grooveshop-storefront-ui-node-nuxt",
	"version": "1.6.1",
	"private": false,
	"description": "Grooveshop Storefront UI Node Nuxt",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/vasilistotskas/grooveshop-storefront-ui-node-nuxt.git"
	},
	"author": "vasilistotskas",
	"license": "MIT",
	"scripts": {
		"build": "nuxi build",
		"dev": "nuxi dev",
		"prepare": "nuxi prepare",
		"generate": "nuxi generate",
		"preview": "nuxi preview",
		"start": "node .output/server/index.mjs",
		"serve": "serve dist/",
		"lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
		"lint:fix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .eslintignore .",
		"format": "prettier --check --ignore-path .gitignore .",
		"format:fix": "prettier --write --ignore-path .gitignore .",
		"stylelint": "stylelint \"**/*.{vue,css,scss}\" --ignore-path .stylelintignore",
		"stylelint:fix": "stylelint \"**/*.{vue,css,scss}\" --fix --ignore-path .stylelintignore",
		"clean": "nuxi clean",
		"cleanup": "nuxi cleanup",
		"generate:locales": "ts-node --esm tools/translator.ts ./locales en-US.yml",
		"analyze": "npx nuxi analyze",
		"test:dev": "NUXT_VITEST_DEV_TEST=true nuxt dev",
		"test:unit": "vitest",
		"test:types": "nuxi prepare && vue-tsc --noEmit",
		"test": "npm test:unit --run && npm test:dev && npm test:types",
		"semantic-release": "semantic-release"
	},
	"devDependencies": {
		"@babel/core": "^7.22.10",
		"@headlessui/vue": "^1.7.16",
		"@iconify/json": "^2.2.105",
		"@nuxt/content": "^2.7.2",
		"@nuxt/devtools": "^0.8.0",
		"@nuxt/image": "rc",
		"@nuxt/types": "^2.17.1",
		"@nuxtjs/eslint-config": "^12.0.0",
		"@nuxtjs/eslint-config-typescript": "^12.0.0",
		"@nuxtjs/eslint-module": "^4.1.0",
		"@nuxtjs/html-validator": "^1.5.2",
		"@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge",
		"@nuxtjs/stylelint-module": "^5.1.0",
		"@nuxtjs/tailwindcss": "^6.8.0",
		"@pinia/nuxt": "^0.4.11",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^10.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^9.0.4",
		"@semantic-release/npm": "^10.0.5",
		"@semantic-release/release-notes-generator": "^11.0.4",
		"@sindresorhus/slugify": "^2.2.1",
		"@tailwindcss/typography": "0.5.9",
		"@testing-library/jest-dom": "^6.1.1",
		"@types/js-yaml": "^4.0.5",
		"@types/node": "^20.5.4",
		"@types/tailwindcss": "^3.1.0",
		"@types/uuid": "^9.0.2",
		"@vee-validate/nuxt": "^4.11.3",
		"@vee-validate/zod": "^4.11.3",
		"@vite-pwa/nuxt": "^0.1.0",
		"@vuepic/vue-datepicker": "^6.0.1",
		"@vueuse/core": "^10.3.0",
		"@vueuse/nuxt": "^10.3.0",
		"autoprefixer": "^10.4.15",
		"cookie-es": "^1.0.0",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-nuxt": "^4.0.0",
		"eslint-plugin-prettier": "^v5.0.0",
		"js-yaml": "^4.1.0",
		"lint-staged": "^14.0.1",
		"lottie-web": "^5.12.2",
		"magic-string": "^0.30.3",
		"nuxt": "3.6.5",
		"nuxt-link-checker": "^2.1.1",
		"nuxt-lodash": "^2.5.0",
		"nuxt-og-image": "^2.0.18",
		"nuxt-schema-org": "^2.2.0",
		"nuxt-seo-experiments": "^3.1.1",
		"nuxt-simple-robots": "^3.1.1",
		"nuxt-simple-sitemap": "^3.2.5",
		"nuxt-vitest": "^0.10.2",
		"pinia": "^2.1.6",
		"playwright": "^1.37.1",
		"postcss": "8.4.28",
		"postcss-html": "^1.5.0",
		"postcss-loader": "^7.3.3",
		"prettier": "^3.0.2",
		"sass": "1.66.1",
		"sass-loader": "^13.3.2",
		"semantic-release": "^21.1.0",
		"serve": "^14.2.1",
		"string-replace-loader": "3.1.0",
		"stylelint-config-recommended-vue": "^1.5.0",
		"stylelint-config-standard": "^34.0.0",
		"stylelint-config-standard-scss": "^10.0.0",
		"sweetalert2": "^11.7.27",
		"tailwindcss": "^3.3.3",
		"translate": "^2.0.2",
		"tsc-alias": "^1.8.7",
		"typescript": "^5.1.6",
		"unplugin-auto-import": "^0.16.6",
		"unplugin-icons": "^0.16.5",
		"unplugin-vue-components": "^0.25.1",
		"uuid": "^9.0.0",
		"vee-validate": "^4.11.3",
		"vite": "^4.4.9",
		"vitest": "^0.33.0",
		"vue": "^3.3.4",
		"vue-toastification": "^2.0.0-rc.5",
		"vue-tsc": "^1.8.8",
		"webpack": "^5.88.2",
		"zod": "^3.22.2",
		"postcss-import": "15.1.0"
	},
	"publishConfig": {
		"@vasilistotskas:registry": "https://npm.pkg.github.com"
	},
	"release": {
		"branches": [
			"main"
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			"@semantic-release/changelog",
			"@semantic-release/npm",
			[
				"@semantic-release/git",
				{
					"assets": [
						"CHANGELOG.md",
						"package.json"
					],
					"message": "chore(release): ${nextRelease.version}\n\n${nextRelease.notes}"
				}
			],
			[
				"@semantic-release/github",
				{
					"assets": [
						{
							"path": ".nuxt/dist/client",
							"label": "Client"
						},
						{
							"path": ".nuxt/dist/server",
							"label": "Server"
						},
						{
							"path": ".output",
							"label": "Output"
						}
					]
				}
			],
			[
				"@semantic-release/npm",
				{
					"npmPublish": false,
					"tarballDir": ".output"
				}
			]
		]
	},
	"lint-staged": {
		"**/*.{js,ts,vue,html}": [
			"npm lint:fix",
			"npm format:fix"
		]
	},
	"engines": {
		"node": ">=18.16.0",
		"npm": ">=9.5.1"
	}
}
