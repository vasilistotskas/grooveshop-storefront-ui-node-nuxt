  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:all": "ncu -u && pnpm i && pnpm run build && pnpm run lint:fix && pnpm run dev",
    "prepare": "nuxt prepare",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "start": "node .output/server/index.mjs",
    "serve": "serve dist/",
    "lint:fix": "eslint . --fix",
    "cleanup": "nuxt cleanup",
    "generate:locales": "tsx ./tools/translator/src/commands/translate.ts --inputFile locales/en-US.yml --selectedLocales en-US,el-GR --outputExtension yml",
    "analyze": "npx nuxt analyze",
    "test:dev": "nuxt dev --no-fork NUXT_VITEST_DEV_TEST=true",
    "test:unit": "vitest run",
    "test": "pnpm test:unit --run && pnpm test:types",
    "semantic-release": "semantic-release"
  },
